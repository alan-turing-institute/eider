{
    "source_file": ["random_smr04_data.csv"],
    "transformation_type": ["COUNT"],
    "grouping_columns": ["user_id", "cis_marker"],
    "absent_default_value": [0],
    "output_feature_name": ["user_defined_name"],
    "primary_filter" : {
        "type": ["AND"],
        "subfilters":{
            "subfilter_1": {
                "column": ["paid_date"],
                "type": ["DATE_GT_EQ"],
                "value": ["2015-01-01"]
            },
            "subfilter_2": {
                "column": ["paid_date"],
                "type": ["DATE_LT_EQ"],
                "value": ["2018-01-01"]
            },
            "subfilter_3": {
                "column": ["episode_within_cis"],
                "type": ["IN"],
                "value": [1]
            }
        }
    }
}
