{
    "source_file": ["../example_data/random_pis_data.csv"],
    "transformation_type": ["SUM"],
    "aggregation_column": ["num_items"],
    "grouping_columns": ["id"],
    "absent_data_flag": [0],
    "output_feature_name": ["number_of_prescribed_items_goes_here"],
    "primary_filter" : {
        "type": ["AND"],
        "subfilters":{
            "subfilter_1": {
                "column": ["paid_date"],
                "type": ["DATE_GT_EQ"],
                "value": ["2015-01-01"]
            },
            "subfilter_2": {
                "column": ["paid_date"],
                "type": ["DATE_LT_EQ"],
                "value": ["2018-01-01"]
            }
        }
    }
}
