{
    "source_file": ["random_pis_data.csv"],
    "transformation_type": ["SUM"],
    "aggregation_column": ["num_items"],
    "grouping_columns": ["user_id"],
    "absent_data_flag": [0],
    "output_feature_name": ["user_defined_name"],
    "primary_filter" : {
        "type": ["AND"],
        "subfilters":{
            "subfilter_1": {
                "column": ["bnf_section"],
                "type": ["IN"],
                "value": ["0203", "0204"]
            },
            "subfilter_2": {
                "column": ["paid_date"],
                "type": ["DATE_GT_EQ"],
                "value": ["2015-01-01"]
            },
            "subfilter_3": {
                "column": ["paid_date"],
                "type": ["DATE_LT_EQ"],
                "value": ["2018-01-01"]
            }
        }
    }
}
